{
  "name": "assert-diff",
  "description": "Drop-in replacement for assert to give diff on deepEqual.",
  "keywords": [
    "assert",
    "diff",
    "deepEqual",
    "object",
    "array",
    "strict"
  ],
  "author": "Antti Mattila <pihvi@live.com>",
  "repository": "git://github.com/pihvi/assert-diff",
  "version": "2.0.0",
  "main": "./lib",
  "license": "Apache-2.0",
  "dependencies": {
    "assert-plus": "1.0.0",
    "json-diff": "0.5.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.55",
    "@babel/core": "^7.0.0-beta.55",
    "@babel/plugin-transform-modules-commonjs": "^7.0.0-beta.55",
    "mocha": "5.2.0",
    "prettier": "^1.14.0"
  },
  "scripts": {
    "test": "mocha test/example-passing-tests.js",
    "start": "npm test",
    "format": "prettier \"{lib,test}/*.mjs\"",
    "build": "babel lib/index.mjs --out-file lib/index.js --plugins=@babel/transform-modules-commonjs"
  }
}
